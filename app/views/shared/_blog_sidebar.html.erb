<div class="col-sm-3 offset-sm-1 blog-sidebar">
  <div class="sidebar-module sidebar-module-inset">
    <h4>About</h4>
    
  </div>
  <div class="sidebar-module">
    <h4>Archives</h4>
    <ol class="list-unstyled">
      <li><%=link_to prevmonth.strftime("%B"),{:controller => "blogs",:action => "archives",:date => prevmonth},:method=> :get%>
      (<%= blogcountlastmonth %>) </li>
      <li><%=link_to prevmonth.prev_month.strftime("%B"),{:controller => "blogs",:action => "archives",:date => prevmonth.prev_month},:method=> :get%> (<%= blogcountpenultimate%>) </li>
      <!-- <li><a href="/archives"><%# Date.today.prev_month.strftime("%B")%> (<%# archivescount[0].count %>)</a></li>
      <li><a href="/archives"><%# Date.today.prev_month.prev_month.strftime("%B")%> (<%# archivescount[1].count %>)</a></li> -->      
    </ol>
  </div>

  <div class="sidebar-module">
    <h4>Elsewhere</h4>
    <ol class="list-unstyled social-links">
      <li><a href="https://github.com/"><%= fa_icon "github" %></a></li>
      <li><a href="https://www.facebook.com/"><%= fa_icon "facebook" %></a></li>
      <li><a href="https://twitter.com/login"><%= fa_icon "twitter" %></a></li>
      <li><a href="https://www.instagram.com/"><%= fa_icon "instagram" %></a></li>
    </ol>
  </div>
</div>